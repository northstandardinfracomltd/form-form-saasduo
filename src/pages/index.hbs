<script>
function formatPhoneNumber(input) {
  let phoneNumber = input.value.replace(/\D/g, '');
  phoneNumber = phoneNumber.replace(/(\d{3})(?=\d)/g, '$1 ');
  phoneNumber = phoneNumber.substring(0, 15);
  input.value = phoneNumber;
}
</script>


            
